	<queryString>
		<![CDATA[select b.codigo, b.descripcion,b.tipocuenta,
a.saldoFinal,
IF(b.tipocuenta = 4, a.saldoFinal, 0) as debe,
IF(b.tipocuenta <> 4, a.saldoFinal, 0) as haber

 from saldo a
inner join cuenta b on b.idcuenta = a.idcuenta
where anio = $P{anio} and b.tipocuenta IN(3,4)
order by b.tipocuenta, b.codigo]]>
